    "dev": "nodemon --exec \"npm run test\" --watch lib --watch tests -e ts,tsx"
  },
  "devDependencies": {
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "codecov": "^3.8.3",
    "ioredis": "^4.28.3",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "ioredis": ">=4.x"
  },
  "jest": {
    "roots": [
      "<rootDir>/tests"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
